<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#a3a3a3"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Fragment Home"
                android:textSize="22dp" />

            <Button
                android:id="@+id/start_activity_c"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="startActivity C"
                android:textSize="25dp" />

            <Button
                android:id="@+id/start_activity_async"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="startAsyncHttp"
                android:textSize="25dp" />

            <Button
                android:id="@+id/start_swipe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="startSipe"
                android:textSize="25dp" />

            <Button
                android:id="@+id/start_image_loader"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="startLoadImage"
                android:textSize="25dp" />

            <Button
                android:id="@+id/start_reg_exp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="startRegExp"
                android:textSize="25dp" />

            <Button
                android:id="@+id/start_car_act"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="startCarAct"
                android:textSize="25dp" />

            <Button
                android:id="@+id/start_demical_format"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START DEMICAL FORMAT"
                android:textSize="25sp" />

            <Button
                android:id="@+id/start_webview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START web_view"
                android:textSize="25sp" />

            <Button
                android:id="@+id/start_progress_bar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START progress_bar"
                android:textSize="25sp" />

            <Button
                android:id="@+id/start_shape"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START shape"
                android:textSize="25sp" />

            <Button
                android:id="@+id/start_touchevent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START Touch_Event"
                android:textSize="25sp" />

            <Button
                android:id="@+id/start_todo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START Todo"
                android:textSize="25sp" />

            <Button
                android:id="@+id/start_hud"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START hud"
                android:textSize="25sp" />

            <Button
                android:id="@+id/start_tween_anmi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START tween_anmi"
                android:textSize="25sp" />

            <Button
                android:id="@+id/start_ui_element"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="START ui_element"
                android:textSize="25sp" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>